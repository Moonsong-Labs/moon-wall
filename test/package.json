    "@moonwall/types": "workspace:*",
    "@moonwall/util": "workspace:*",
    "@openzeppelin/contracts": "4.9.3",
    "@polkadot/api": "10.11.2",
    "@polkadot/api-augment": "10.11.2",
    "@polkadot/util": "12.6.2",
    "@types/node": "20.10.8",
    "@vitest/ui": "1.1.3",
    "bun-types": "1.0.22",
    "chai": "5.0.0",
    "chalk": "5.3.0",
    "ethers": "6.9.2",
    "pnpm": "8.14.0",
    "prettier": "3.1.1",
    "solc": "0.8.23-fixed",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "viem": "2.0.5",
