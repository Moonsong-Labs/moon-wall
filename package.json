{
  "name": "moonwall",
  "version": "1.0.0",
  "description": "Testing framework for the Moon family of projects",
  "main": "./dist/src/index.js",
  "scripts": {
    "compile": "rm -rf dist/* && tsc ",
    "start": "node -r ts-node/register dist/src/cli/index.js",
    "lint": "npx prettier --write --ignore-path .gitignore '**/*.(yml|js|ts|json)'",
    "test-bin": "npm run compile && npm run start -- test -f tests/*.ts",
    "new-cmd": "DEBUG=smoke* npx ts-node -r ts-node/register ./src/cli/index.ts test -t New_Test",
    "new-cmd-js": "npm run compile && DEBUG=smoke* npm run start -- test -t New_Test"
  },
  "bin": {
    "moonwall": "./src/cli/index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@moonbeam-network/api-augment": "^0.2000.1",
    "@polkadot/api": "^9.13.6",
    "@polkadot/api-augment": "^9.13.6",
    "@polkadot/api-derive": "^9.13.6",
    "@polkadot/apps-config": "^0.122.2",
    "@polkadot/keyring": "^10.3.1",
    "@polkadot/types": "^9.13.6",
    "@polkadot/types-codec": "^9.13.6",
    "@polkadot/util": "^10.3.1",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.13.0",
    "@types/yargs": "^17.0.22",
    "api:9.9.4": "link:@polkadpt/api:9.9.4",
    "bottleneck": "^2.19.5",
    "chai": "^4.3.7",
    "chalk": "^5.2.0",
    "debug": "^4.3.4",
    "ethers": "^6.0.2",
    "mocha": "^10.2.0",
    "moment-timezone": "^0.5.40",
    "moonbeam-types-bundle": "^2.0.9",
    "prettier": "^2.8.4",
    "semver": "^7.3.8",
    "ts-node": "^10.9.1",
    "types": "link:@polkadot/types-codec/types",
    "typescript": "^4.9.5",
    "web3": "4.0.1-alpha.5",
    "web3-types": "0.1.1-alpha.4",
    "yargs": "^17.6.2"
  },
  "overrides": {
    "@polkadot/api": "$@polkadot/api",
    "@polkadot/api-derive": "$@polkadot/api",
    "@polkadot/api-augment": "$@polkadot/api",
    "@polkadot/api-base": "$@polkadot/api",
    "@polkadot/rpc-augment": "$@polkadot/api",
    "@polkadot/rpc-core": "$@polkadot/api",
    "@polkadot/rpc-provider": "$@polkadot/api",
    "@polkadot/types-create": "$@polkadot/api",
    "@polkadot/types-augment": "$@polkadot/api",
    "@polkadot/types-support": "$@polkadot/api",
    "@polkadot/types-codec": "$@polkadot/api",
    "@polkadot/types": "$@polkadot/api",
    "@polkadot/types-known": "$@polkadot/api",
    "@polkadot/util": "$@polkadot/util",
    "@polkadot/util-crypto": "$@polkadot/util"
  }
}
